{
  "name": "site_efsvp",
  "version": "1.0.0",
  "type": "module",
  "description": "Site web one-page professionnel pour EfSVP, studio de création narrative et musicale basé à Angers.",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.js' --max-warnings=0",
    "lint:fix": "eslint 'src/**/*.js' --fix",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:css:fix": "stylelint 'src/**/*.css' --fix",
    "format": "prettier --write 'src/**/*.{js,css}' 'index.html'",
    "format:check": "prettier --check 'src/**/*.{js,css}' 'index.html'",
    "audit:code": "npm run lint && npm run lint:css && npm run format:check",
    "type-check": "tsc --noEmit",
    "projects:parse": "node --experimental-strip-types scripts/parse-projects.ts",
    "projects:validate": "npm run projects:parse",
    "test": "node --experimental-strip-types --test scripts/parse-projects.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "http://local_proxy@127.0.0.1:26592/git/lesaffrejb-beep/Site_eFsvp"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "vite": "^7.1.12"
  },
  "dependencies": {
    "gsap": "^3.13.0",
    "splitting": "^1.1.0",
    "swiper": "^12.0.3",
    "three": "^0.181.1",
    "wavesurfer.js": "^7.11.1"
  }
}
